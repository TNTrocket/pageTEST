    <script type="text/javascript">
    function getUrlParam(name) {
      var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
      var r = window.location.search.substr(1).match(reg);
      if (r != null) return unescape(r[2]);
      return null;
    }
    $(function() {
      dropdownOpen();
      $('div.attentionUs').hover(function() {
        $('img.twocode').css('display', 'block');
      }, function() {

        $('img.twocode').css('display', 'none');
      })
      $(".nav-justified .all-menu ul.dropdown-menu li").hover(function() {
        $(this).addClass('active');
      }, function() {
        $(this).removeClass('active');
      });
      $('.receivedBox .sureBtn, .receivedBox .closeBtn').click(function() {
        $('.receivedBox').hide();
        $('.bg').hide();
        $('.tip', '.helpFindTeacherBox').text('');
      });

      $('.findTeacherForm').click(function() {
        var username = $('input[name="userName"]', '.modal-find-teacher-body').val();
        var phone = $('input[name="userPhone"]', '.modal-find-teacher-body').val();
        var areaname = $('input[name="userArea"]', '.modal-find-teacher-body').val();
        var remark = '';
        var grade = "0";
        if (username == '') {
          alert('请填写您的姓名');
          //$('.tip', '.helpFindTeacherBox').text('请填写您的姓名');
          return;
        }

        if (areaname == '') {
          alert('请填地区');
          //$('.tip', '.helpFindTeacherBox').text('请填写需要学习的科目');
          return;
        }

        if (phone == '') {
          alert('请填写您的手机号码');
          //$('.tip', '.helpFindTeacherBox').text('请填写您的手机号码');
          return;
        }

        var phoneReg = /^1(3|4|5|6|7|8)\d{9}$/;
        if (!phoneReg.test(phone)) {
          alert('请填写正确的手机号码');
          //$('.tip', '.helpFindTeacherBox').text('请填写正确的手机号码');
          return;
        }
        $.ajax({
          type: "POST",
          url: '/web/subteacher',
          data: {
            'phone': phone,
            'checksum': 'mobile',
            'gradeCourse': areaname,
            'remark': '',
            'realname': username,
            'areaId': 440100,
            'channel': getUrlParam("channel"),
            'grade': '0'
          },
          complete: function(e, xhr, settings) {
            if (e.status === 200) {
              alert("提交成功")
                // $('.receivedBox, .bg').show();
              $('input,textarea', '.modal-find-teacher-body').val('');
              //   $('select option:eq(0)', '.bottomSearchBox').attr('selected', 'selected');
              //   $('.helpFindTeacherBox.pop').hide();
            } else {
              var rep = JSON.parse(e.responseText);
              alert(rep.error);
            }
          }
        });
      })
      $('.immediateReservation').click(function() {
        var course = $('input[name="course"]', '.bottomSearchBox').val();
        var phone = $('input[name="phone"]', '.bottomSearchBox').val();
        var realname = $('input[name="realname"]', '.bottomSearchBox').val();
        var remark = '';
        var grade = "0";

        if (realname == '') {
          alert('请填写您的姓名');
          //$('.tip', '.helpFindTeacherBox').text('请填写您的姓名');
          return;
        }

        if (course == '') {
          alert('请填写');
          //$('.tip', '.helpFindTeacherBox').text('请填写需要学习的科目');
          return;
        }

        if (phone == '') {
          alert('请填写您的手机号码');
          //$('.tip', '.helpFindTeacherBox').text('请填写您的手机号码');
          return;
        }

        var phoneReg = /^1(3|4|5|6|7|8)\d{9}$/;
        if (!phoneReg.test(phone)) {
          alert('请填写正确的手机号码');
          //$('.tip', '.helpFindTeacherBox').text('请填写正确的手机号码');
          return;
        }

        $('.tip', '.bottomSearchBox').text('');
        $.ajax({
          type: "POST",
          url: '/web/subteacher',
          data: {
            'phone': phone,
            'checksum': 'mobile',
            'gradeCourse': course,
            'remark': remark,
            'realname': realname,
            'areaId': 440100,
            'grade': grade,
            'channel': getUrlParam("channel")
          },
          complete: function(e, xhr, settings) {
            if (e.status === 200) {
              $('.receivedBox, .bg').show();
              $('input,textarea', '.bottomSearchBox').val('');
              $('select option:eq(0)', '.bottomSearchBox').attr('selected', 'selected');
              $('.helpFindTeacherBox.pop').hide();
            } else {
              var rep = JSON.parse(e.responseText);
              alert(rep.error);
            }
          }
        });
      });
    });

    function dropdownOpen() {
      var $dropdownLi = $('li.dropdown');

      $dropdownLi.mouseover(function() {
        $(this).addClass('open');
      }).mouseout(function() {
        $(this).removeClass('open');
      });
    }
    </script>
